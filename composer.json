{
    "name":"bright-cloud-studio/zyppy-weighted-search",
    "description":"Extends the Zyppy Search package to allow adding weight to entries on the Search Index so they appear earlier in search results",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "zyppy", "weighted", "search", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"GPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13",
        "asconsulting/zyppy_search": "dev-master"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/zyppy_weighted_search/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/zyppy_weighted_search": "system/modules/zyppy_weighted_search"
            }
        }
    },
    "minimum-stability": "dev"
}
